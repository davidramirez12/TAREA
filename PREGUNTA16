print("Bienvenido estudiante, por favor ingrese su nombre.")
nomb=input()
print("Estudiante", nomb , ",Ingrese la cantidad desde donde quiere que se comience a imprimir los numeros" )
n=int(input())
print("Estudiante", nomb  , ",Ingrese el numero hasta donde se imprimira dicho numero" )
a=int(input())

for i in range (n,a+1): 
    print("La lista de numeros sera")
    print( i)
